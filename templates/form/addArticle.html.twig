{{ form_start(formAddArticle) }}
    <div class="container">
        <div id="" class="row">
            <label for="type-article-selector"></label>
            <select id="type-article-selector" class="custom-select">
                <option selected></option>
                <option value="1">Contact</option>
                <option value="2">Entreprise</option>
                <option value="3">Projet</option>
            </select>
            <div id="proj-inf" class="col-md">
                {{ form_row(formAddArticle.ProjName) }}
                <div>
                    <label for="projDateStart">Date de d√©but du projet</label>
                    <div>
                        <input type="date" id="projDateStart" class="form-control" name="date-start">
                    </div>
                </div>
                <div>
                    <label for="projDateEnd">Date de fin du projet</label>
                    <div>
                        <input type="date" id="projDateEnd" class="form-control" name="date-end">
                    </div>
                </div>
                {{ form_row(formAddArticle.ProjStatut) }}
            </div>
            <div id="proj-inf2" class="col-md">
